{
  "name": "ElectronJs",
  "version": "0.1.0",
  "private": true,
  "author": "Hari",
  "description": "Monaco-Electron app",
  "homepage": "./",
  "main": "./public/electron.js",
  "build": {
    "appId": "com.electron.myapp",
    "productName": "Monaco-Electron App",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "src/**/*",
      "public/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@monaco-editor/react": "^4.4.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "clipboard": "^2.0.11",
    "electron-rebuild": "^3.2.9",
    "node-pty": "^0.10.1",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-folder-tree": "^5.1.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "xterm": "^5.1.0",
    "xterm-addon-attach": "^0.8.0"
  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "rebuild": "electron-rebuild",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3001 && electronmon .\"",
    "electron:package:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/standalone": "^7.21.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2",
    "http-proxy-middleware": "^2.0.6",
    "wait-on": "^7.0.1",
    "xterm-addon-fit": "^0.7.0"
  },
  "keywords": [],
  "license": "ISC"
}
